Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"SetRamRestriction", "::", "usage"}], " ", "=", " ", 
   "\"\<SetRamRestriction[maxMemAllowedGB_:2,intervalBetweenTests_:5]\n\tRun \
a task that checks every intervalBetweenTests the amount of memory used by \
Mathematica kernel and aborts it if the limit is exceeded. \n\tThe function \
is used to prevent unlimited memory allocation by Mathematica that arises \
with certain programming mistakes when processing \n\tlarge amounts of data \
and irreversibly hangs the entire computer.\n\tThe function uses global \
variable ramBelowThresholdSignal as counter.\n\nOutput:\n\t\
ScheduledTaskObject which can be used stop the memory monitoring by \
RemoveScheduledTask[]\>\""}], "\n"}], "\n", 
 RowBox[{
  RowBox[{"Begin", "[", "\"\<`Private`\>\"", "]"}], "\n"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"SetRamRestriction", "[", 
    RowBox[{
     RowBox[{"maxMemAllowedGB_:", "2"}], ",", 
     RowBox[{"intervalBetweenTests_:", "5"}]}], "]"}], ":=", 
   RowBox[{"(", "\n", "\t", 
    RowBox[{
     RowBox[{"ramBelowThresholdSignal", "=", "0"}], ";", "\n", "\t", 
     RowBox[{"Print", "[", 
      RowBox[{"Dynamic", "[", "ramBelowThresholdSignal", "]"}], "]"}], ";", 
     "\n", "\t", 
     RowBox[{"RunScheduledTask", "[", 
      RowBox[{
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"MemoryInUse", "[", "]"}], ">", 
          RowBox[{"(", 
           RowBox[{"maxMemAllowedGB", " ", 
            RowBox[{"1024", "^", "3"}]}], ")"}]}], ",", 
         RowBox[{"Quit", "[", "]"}], ",", 
         RowBox[{"ramBelowThresholdSignal", "++"}]}], "]"}], ",", 
       "intervalBetweenTests"}], "]"}]}], "\n", ")"}]}], "\n"}], "\n", 
 RowBox[{"End", "[", "]"}]}], "Code",
 CellChangeTimes->{{3.7382255882493253`*^9, 3.738225627562111*^9}, {
