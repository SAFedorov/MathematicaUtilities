           RowBox[{"UpperLine", "[", 
            RowBox[{"i", ",", 
             RowBox[{"traceList", "[", 
              RowBox[{"[", 
               RowBox[{"i", ",", 
                RowBox[{"-", "1"}], ",", "1"}], "]"}], "]"}]}], "]"}]}], 
          "}"}], ",", "\n", "\t", 
         RowBox[{"{", 
          RowBox[{"i", ",", 
           RowBox[{"Length", "[", "traceList", "]"}]}], "}"}]}], "]"}]}], ";",
       "\n", "\t", "\n", "\t", 
      RowBox[{"Clear", "[", "fitRanges", "]"}], ";", "\n", "\t", 
