Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"AverageXYLists", "::", "usage"}], "=", 
   "\"\<AverageXYLists[xylistd2_]\n\tComputes average of y values for a set \
of xylists. Lists are aligned by the first element and trimmed to the minimum \
length among them. \n\t\>\""}], "\n"}], "\n", 
