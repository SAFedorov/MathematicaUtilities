  "\n"}], "\n", 
 RowBox[{
  RowBox[{"Begin", "[", "\"\<`Private`\>\"", "]"}], "\n"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"FitRangeSelector", "[", 
    RowBox[{"fitRanges_", ",", "traceList_", ",", 
     RowBox[{"OptionsPattern", "[", 
      RowBox[{"{", "FitRangeSelector", "}"}], "]"}]}], "]"}], ":=", 
   RowBox[{"DynamicModule", "[", 
