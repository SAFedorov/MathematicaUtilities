Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"FindPeakCenter", "::", "usage"}], "=", 
   "\"\<FindPeakCenter[list_]\n\tfind x-position of the center of mass of \
xylist using linear data interpolation over x\n\nInput:\n\tlist_ is xylist or \
xylistd2\n\nOptions:\n\tPeakSign\[Rule]1, switches between looking for peak \
(1) or valley (-1)\>\""}], "\n"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"Options", "[", "FindPeakCenter", "]"}], "=", 
   RowBox[{"{", 
    RowBox[{"PeakSign", "\[Rule]", "1"}], "}"}]}], "\n"}], "\n", 
 RowBox[{
  RowBox[{"Begin", "[", "\"\<`Private`\>\"", "]"}], "\n"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"FindPeakCenter", "[", 
    RowBox[{
     RowBox[{"xylist_", "?", "XYListQ"}], ",", 
     RowBox[{"opts", ":", 
      RowBox[{"OptionsPattern", "[", "]"}]}]}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "offset", ",", "yAccList", ",", "yInterpIntegral", ",", "indCOM"}], 
      "}"}], ",", "\n", "\t", 
     RowBox[{"(*", 
      RowBox[{
      "offset", " ", "is", " ", "subtracted", " ", "for", " ", "more", " ", 
       "robust", " ", "peak", " ", "search"}], "*)"}], "\n", "\t", 
     RowBox[{
      RowBox[{"offset", "=", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"OptionValue", "[", "PeakSign", "]"}], ">=", "0"}], ",", 
         " ", 
         RowBox[{"Min", "[", 
          RowBox[{"xylist", "[", 
           RowBox[{"[", 
            RowBox[{";;", ",", "2"}], "]"}], "]"}], "]"}], ",", " ", 
         RowBox[{"Max", "[", 
          RowBox[{"xylist", "[", 
           RowBox[{"[", 
            RowBox[{";;", ",", "2"}], "]"}], "]"}], "]"}]}], "]"}]}], ";", 
      "\t", "\n", "\t", 
      RowBox[{"yAccList", "=", 
       RowBox[{"Accumulate", "[", 
        RowBox[{
         RowBox[{"xylist", "[", 
          RowBox[{"[", 
           RowBox[{";;", ",", "2"}], "]"}], "]"}], "-", "offset"}], "]"}]}], 
      ";", "\t", "\n", "\t", 
      RowBox[{"yInterpIntegral", "=", 
       RowBox[{"Interpolation", "[", 
        FractionBox["yAccList", 
         RowBox[{"yAccList", "[", 
          RowBox[{"[", 
           RowBox[{"-", "1"}], "]"}], "]"}]], "]"}]}], ";", "\t", "\n", "\t", 
      "\n", "\t", 
      RowBox[{"(*", 
       RowBox[{
        RowBox[{
        "initial", " ", "approximation", " ", "is", " ", "chosen", " ", "to", 
         " ", "be", " ", "in", " ", "the", " ", "interval", " ", "center"}], 
        ",", " ", "\n", "\t", 
        RowBox[{
        "but", " ", "it", " ", "should", " ", "not", " ", "play", " ", "role",
          " ", "if", " ", "the", " ", "peak", " ", "is", " ", "single", " ", 
         "in", " ", "the", " ", "supplied", " ", "xylist"}]}], "*)"}], "\t", 
      "\n", "\t", 
      RowBox[{"(*", 
       RowBox[{
        RowBox[{"indCOM", " ", "-", " ", "index"}], ",", " ", 
        RowBox[{"non", " ", "necessarily", " ", "integer"}], ",", " ", 
        RowBox[{"of", " ", "the", " ", "center", " ", "of", " ", 
         RowBox[{"mass", ".", " ", "It"}], " ", "is", " ", "better", " ", 
         "to", " ", "find", " ", "the", " ", "peak", " ", "position", " ", 
         "in", " ", "dimensionless", " ", "\n", "\t", "units", " ", "first", 
         " ", "not", " ", "to", " ", "suffer", " ", "from", " ", "numerical", 
         " ", "errors", " ", "at", " ", "small", " ", "absolute", " ", 
         "values", " ", "of", " ", "x"}]}], "*)"}], "\n", "\t", 
      RowBox[{"(*", 
       RowBox[{
        RowBox[{
        "0.5", " ", "is", " ", "added", " ", "to", " ", "indCOM", " ", 
         "explicitly", " ", "in", " ", "order", " ", "to", " ", "remove", " ",
          "bias"}], ",", " ", 
        RowBox[{"introduced", " ", "by", " ", "Accumulate"}]}], "*)"}], "\n", 
      "\t", 
      RowBox[{"(*", 
       RowBox[{
        RowBox[{
        "Also", " ", "use", " ", "Quiet", " ", "for", " ", "the", " ", 
         "numerical", " ", "routine", " ", "not", " ", "to", " ", "complain", 
         " ", "about", " ", "slow", " ", "convergence"}], ",", " ", 
        RowBox[{
        "there", " ", "is", " ", "little", " ", "can", " ", "be", " ", "done",
          " ", "about", " ", "it"}]}], "*)"}], "\n", "\t", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"indCOM", "=", 
         RowBox[{"0.5", "+", 
          RowBox[{"(", 
           RowBox[{"x", "/.", 
            RowBox[{"FindRoot", "[", 
             RowBox[{
              RowBox[{
               RowBox[{"yInterpIntegral", "[", "x", "]"}], "==", "0.5"}], ",", 
              RowBox[{"{", 
               RowBox[{"x", ",", 
                RowBox[{
                 RowBox[{"Length", "[", "yAccList", "]"}], "/", "2"}]}], 
               "}"}]}], "]"}]}], ")"}]}]}], ")"}], "//", "Quiet"}], ";", "\n",
       "\t", "\n", "\t", 
      RowBox[{"(*", 
       RowBox[{
       "Convert", " ", "indCOM", " ", "to", " ", "x", " ", "using", " ", 
        "linear", " ", "interpolation"}], "*)"}], "\n", "\t", 
      FractionBox[
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          RowBox[{"xylist", "[", 
           RowBox[{"[", 
            RowBox[{
             RowBox[{"Floor", "[", "indCOM", "]"}], ",", "1"}], "]"}], "]"}], 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"Ceiling", "[", "indCOM", "]"}], "-", "indCOM"}], ")"}]}],
          "+", 
         RowBox[{
          RowBox[{"xylist", "[", 
           RowBox[{"[", 
            RowBox[{
             RowBox[{"Ceiling", "[", "indCOM", "]"}], ",", "1"}], "]"}], 
           "]"}], 
          RowBox[{"(", 
           RowBox[{"indCOM", "-", 
            RowBox[{"Floor", "[", "indCOM", "]"}]}], ")"}]}]}], ")"}], 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"Ceiling", "[", "indCOM", "]"}], "-", 
         RowBox[{"Floor", "[", "indCOM", "]"}]}], ")"}]]}]}], "\n", "]"}]}], 
  "\n", "\n", 
  RowBox[{"(*", 
   RowBox[{"Thread", " ", "over", " ", "first", " ", "argument"}], 
   "*)"}]}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"FindPeakCenter", "[", 
    RowBox[{
     RowBox[{"xylistd2_", "?", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"XYListQ", "[", 
